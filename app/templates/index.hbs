<h1>Welcome to Question and Answer</h1>

<h1>Questions</h1>

<ul>
  {{#each model.questions as |question|}}
    <li>
      <h3>
        {{#link-to 'question' question.id}}{{question.content}}{{/link-to}}
      </h3>
      <p>by {{question.author}}</p>
    </li>
  {{/each}}
</ul>

{{new-question save='save'}}
